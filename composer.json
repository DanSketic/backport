{
    "name": "dansketic/backport",
    "description": "backport",
    "type": "library",
    "keywords": ["laravel", "backport", "backend", "dansketic"],
    "homepage": "https://github.com/dansketic/backport",
    "license": "MIT",
    "authors": [
        {
            "name": "DanSketic",
            "email": "dansketic@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/dom-crawler": "~3.1|~4.0|~5.0",
        "laravel/framework": ">=5.5",
        "doctrine/dbal": "2.*"
    },
    "require-dev": {
        "laravel/laravel": ">=5.5",
        "fzaninotto/faker": "~1.4",
        "intervention/image": "~2.3",
        "laravel/browser-kit-testing": "^6.0",
        "spatie/phpunit-watcher": "^1.22.0"
    },
    "autoload": {
        "psr-4": {
            "DanSketic\\Backport\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "suggest": {
        "intervention/image": "Required to handling and manipulation upload images (~2.3).",
        "spatie/eloquent-sortable": "Required to built orderable gird."
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanSketic\\Backport\\BackportServiceProvider"
            ],
            "aliases": {
                "Backport": "DanSketic\\Backport\\Facades\\Backport"
            }
        }
    }
}
